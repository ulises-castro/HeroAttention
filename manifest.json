{
  "name": "Hero Attention",
  "description": "Hero Attention",
  "version": "0.0.1",
  "manifest_version": 3,
   "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  }, 
  "background": {
    "service_worker": "./background.js"
  },
  "options_page": "./options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://www.google.com/*"
  ]
}
